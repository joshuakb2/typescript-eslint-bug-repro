{
    "compilerOptions": {
        "module": "esnext",
        "moduleResolution": "nodenext",
        "lib": [ "esnext" ],
        "charset": "utf8",
        "rootDir": ".",
        "outDir": "../out",
        "target": "es2017",
        "downlevelIteration": true,
        "sourceMap": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "incremental": true,
        "strict": true,
        "strictPropertyInitialization": false,
        "declaration": true,
        "declarationMap": true,
        "types": ["node"],
        "newLine": "LF",
    },
    "references": []
}
